    "# question 12\n",
    "# for loop to print 1 to 10\n",
    "i = 0\n",
    "for i in range(0,10):\n",
    "    i += 1\n",
    "    print(list(i))\n",
    "#while loop to print 1 to 10\n",
    "i = 1\n",
    "while True:\n",
    "    if i > 10:\n",
    "        break\n",
    "    print(i)\n",
    "    i += 1"
